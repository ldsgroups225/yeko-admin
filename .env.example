# Application Environment Configuration
# ===================================

#
 # Current environment stage for the application.
 # @type {'development' | 'staging' | 'production'}
 # @required
 # @default 'development'
 # @example 'development'
 # Controls environment-specific behavior and optimizations.
#
NODE_ENV=development

#
 # Supabase project URL from your project settings.
 # @type {string}
 # @required
 # @format URL
 # @example 'https://xyzproject.supabase.co'
 # @see https://app.supabase.com/project/_/settings/api
 # Used for connecting to your Supabase instance and making API calls.
#
NEXT_PUBLIC_SUPABASE_URL=<your_supabase_project_url>

#
 # Anonymous/public API key from your Supabase project.
 # @type {string}
 # @required
 # @format UUID
 # @example 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.xxx'
 # @see https://app.supabase.com/project/_/settings/api
 # @security This key is safe to expose in the browser as it has limited permissions
 # Used for unauthenticated operations and initial authentication.
#
NEXT_PUBLIC_SUPABASE_ANON_KEY=<your_supabase_anon_key>

#
 # Full URL of your application.
 # @type {string}
 # @required
 # @format URL
 # @example 'http://localhost:3000' for local development
 # @example 'https://your-app.com' for production
 # Used for authentication callbacks and redirect URLs.
 # Must match the URL configured in your Supabase authentication settings.
#
NEXT_PUBLIC_SITE_URL=<your_site_url>

#
 # Resend API key for sending emails.
 # @type {string}
 # @required
 # @format UUID
 # @example 'your_resend_api_key'
 # @see https://resend.com/docs/api
 # Used for sending emails to users.
#
RESEND_API_KEY=<your_resend_api_key>
RESEND_FROM_EMAIL=<your_resend_from_email>

#
 # Google OAuth Client ID for authentication.
 # @type {string}
 # @required
 # @format UUID
 # @example 'your_google_client_id.apps.googleusercontent.com'
 # @see https://console.cloud.google.com/apis/credentials
 # @security This key is safe to expose in the browser as it's a public identifier
 # Used for Google OAuth authentication flow.
#
NEXT_PUBLIC_GOOGLE_CLIENT_ID=<your_google_client_id>

# Sentry Error Monitoring Configuration
# =====================================

#
 # Sentry DSN for error tracking and performance monitoring.
 # @type {string}
 # @optional
 # @format URL
 # @example 'https://your-dsn@sentry.io/project-id'
 # @see https://sentry.io/settings/projects/
 # @security This DSN is safe to expose in the browser as it only allows sending data to Sentry
 # Used for capturing errors, performance data, and session replays.
 # If not provided, Sentry will be disabled.
#
NEXT_PUBLIC_SENTRY_DSN=<your_sentry_dsn>

#
 # Disable Sentry in specific environments.
 # @type {'true' | 'false'}
 # @optional
 # @default undefined (enabled)
 # @example 'true'
 # Set to 'true' to completely disable Sentry error tracking.
 # Useful for testing environments or when you want to disable monitoring.
#
NEXT_PUBLIC_SENTRY_DISABLED=false

#
 # Sentry organization slug for build-time configuration.
 # @type {string}
 # @optional
 # @example 'your-org-slug'
 # @see https://sentry.io/settings/
 # Used for uploading source maps and release management.
 # Only needed if you want to upload source maps for better error tracking.
#
SENTRY_ORG=<your_sentry_org>

#
 # Sentry project slug for build-time configuration.
 # @type {string}
 # @optional
 # @example 'your-project-slug'
 # @see https://sentry.io/settings/projects/
 # Used for uploading source maps and release management.
 # Only needed if you want to upload source maps for better error tracking.
#
SENTRY_PROJECT=<your_sentry_project>

# Better Stack Logging Configuration
# ==================================

#
 # Better Stack API token for log ingestion.
 # @type {string}
 # @optional
 # @format UUID
 # @example 'your_better_stack_token'
 # @see https://betterstack.com/docs/logs/api/
 # @security Keep this token secure - it allows writing logs to your Better Stack account
 # Used for sending application logs to Better Stack for centralized log management.
 # If not provided, logs will only be written to console.
#
BETTER_STACK_TOKEN=<your_better_stack_token>

#
 # Enable Better Stack logging in development.
 # @type {'true' | 'false'}
 # @optional
 # @default undefined (disabled in development)
 # @example 'true'
 # Set to 'true' to enable Better Stack logging in development environment.
 # By default, Better Stack logging is only enabled in production.
#
BETTER_STACK_ENABLED=false

#
 # Number of log entries to batch before sending to Better Stack.
 # @type {string}
 # @optional
 # @default '10'
 # @example '20'
 # Higher values reduce API calls but increase memory usage and delay.
 # Lower values provide faster log delivery but more API calls.
#
BETTER_STACK_BATCH_SIZE=10

#
 # Time in milliseconds to wait before flushing logs to Better Stack.
 # @type {string}
 # @optional
 # @default '5000'
 # @example '3000'
 # Logs are automatically sent when batch size is reached or this interval expires.
 # Lower values provide faster log delivery but more frequent API calls.
#
BETTER_STACK_FLUSH_INTERVAL=5000

# SEO and Site Verification Configuration
# =======================================

#
 # Google Search Console site verification code.
 # @type {string}
 # @optional
 # @example 'your_google_verification_code'
 # @see https://search.google.com/search-console
 # Used to verify site ownership with Google Search Console.
 # Add this meta tag verification code from Google Search Console.
#
GOOGLE_SITE_VERIFICATION=<your_google_verification_code>

#
 # Yandex Webmaster site verification code.
 # @type {string}
 # @optional
 # @example 'your_yandex_verification_code'
 # @see https://webmaster.yandex.com/
 # Used to verify site ownership with Yandex Webmaster.
#
YANDEX_VERIFICATION=<your_yandex_verification_code>

#
 # Yahoo site verification code.
 # @type {string}
 # @optional
 # @example 'your_yahoo_verification_code'
 # Used to verify site ownership with Yahoo.
#
YAHOO_SITE_VERIFICATION=<your_yahoo_verification_code>

#
 # Bing Webmaster Tools site verification code.
 # @type {string}
 # @optional
 # @example 'your_bing_verification_code'
 # @see https://www.bing.com/webmasters/
 # Used to verify site ownership with Bing Webmaster Tools.
#
BING_SITE_VERIFICATION=<your_bing_verification_code>
